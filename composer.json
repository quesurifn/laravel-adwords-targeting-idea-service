{
    "name": "schulzefelix/laravel-adwords-targeting-idea-service",
    "description": "Laravel wrapper for the AdWords Targeting Idea Service",
    "keywords": [
        "SchulzeFelix",
        "Laravel AdWords",
        "AdWords",
        "Laravel",
        "Targeting Idea Service"
    ],
    "homepage": "https://github.com/schulzefelix/laravel-adwords-targeting-idea-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Schulze",
            "email": "githubissues@schulze.co",
            "homepage": "https://schulze.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.2.0",
        "illuminate/console": "~5.2.0",
        "googleads/googleads-php-lib": "^35.0",
        "schulzefelix/laravel-data-transfer-object": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Quesurifn\\AdWords\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quesurifn\\AdWords\\AdWordsServiceProvider"
            ],
            "aliases": {
                "AdWords": "Quesurifn\\AdWords\\AdWordsFacade"
            }
        }
    }
}
